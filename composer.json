{
    "name": "zoker/filament-multisite",
    "description": "Multisite plugin for laravel with Filament managing",
    "type": "library",
    "require": {
        "laravel/framework": "^12.0",
        "php": "^8.4",
        "lara-zeus/spatie-translatable": "^1.0"
    },
    "require-dev": {
        "orchestra/testbench": "^10.6",
        "larastan/larastan": "^v3.8.0",
        "pestphp/pest": "^3.0",
        "pestphp/pest-plugin-laravel": "^3.0"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Zoker\\FilamentMultisite\\": "src/",
            "Zoker\\FilamentMultisite\\Tests\\": "tests/",
            "Zoker\\FilamentMultisite\\Database\\Factories\\": "database/factories/"
        }
    },
    "authors": [
        {
            "name": "Maksim",
            "email": "zokermm@gmail.com",
            "homepage": "https://github.com/zoker68"
        }
    ],
    "minimum-stability": "dev",
    "extra": {
        "laravel": {
            "providers": [
                "Zoker\\FilamentMultisite\\Providers\\FilamentMultisiteRouteServiceProvider",
                "Zoker\\FilamentMultisite\\Providers\\FilamentMultisiteServiceProvider"
            ]
        },
        "branch-alias": {
            "dev-master": "0-dev"
        }
    },
    "scripts": {
        "analyze": "php -d memory_limit=1G vendor/bin/phpstan analyse",
        "test": "php /Users/maksim/Projects/shop/vendor/bin/paratest_for_phpstorm /Users/maksim/Projects/shop/vendor/phpunit/phpunit/phpunit --configuration /Users/maksim/Projects/shop/phpunit.xml /Users/maksim/Projects/shop/packages/zoker/FilamentMultisite/tests",
        "test-analyze": "composer test && composer analyze"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
